<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivityFragment"
                tools:showIn="@layout/activity_main">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="43dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="J4-00"
                android:id="@+id/textView"
                android:layout_alignParentStart="true"
                android:gravity="center"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="J4-01"
                android:id="@+id/textView2"
                android:gravity="center"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="J4-02"
                android:id="@+id/textView3"
                android:gravity="center"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="J4-03"
                android:id="@+id/textView4"
                android:gravity="center"/>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="io0"
                android:id="@+id/toggleButton1"
                android:layout_below="@+id/textView"
                android:layout_alignEnd="@+id/textView"
                android:layout_alignStart="@+id/textView"/>

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="io1"
                android:id="@+id/toggleButton2"
                android:layout_centerVertical="true"
                android:layout_alignStart="@+id/toggleButton1"
                android:layout_alignEnd="@+id/toggleButton1"/>

            <ToggleButton
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:text="io2"
                android:id="@+id/toggleButton3"
                android:layout_below="@+id/toggleButton2"
                android:layout_alignStart="@+id/toggleButton2"
                android:layout_alignEnd="@+id/toggleButton2"/>

            <ToggleButton
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:text="io3"
                android:id="@+id/toggleButton4"
                android:layout_below="@+id/toggleButton2"
                android:layout_alignStart="@+id/toggleButton2"
                android:layout_alignEnd="@+id/toggleButton2"/>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="J4-06"
                android:id="@+id/textView5"
                android:layout_alignParentStart="true"
                android:gravity="center"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="J4-07"
                android:id="@+id/textView6"
                android:gravity="center"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="J4-08"
                android:id="@+id/textView7"
                android:gravity="center"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="J4-09"
                android:id="@+id/textView8"
                android:gravity="center"/>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="io4"
                android:id="@+id/rToggleJ406"
                android:layout_below="@+id/textView"
                android:layout_alignEnd="@+id/textView"
                android:layout_alignStart="@+id/textView"/>

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="io5"
                android:id="@+id/rToggleJ407"
                android:layout_centerVertical="true"
                android:layout_alignStart="@+id/rToggleJ404"
                android:layout_alignEnd="@+id/rToggleJ404"/>

            <ToggleButton
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:text="io6"
                android:id="@+id/rToggleJ408"
                android:layout_below="@+id/rToggleJ404"
                android:layout_alignStart="@+id/rToggleJ404"
                android:layout_alignEnd="@+id/rToggleJ404"/>

            <ToggleButton
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:text="io7"
                android:id="@+id/rToggleJ409"
                android:layout_below="@+id/rToggleJ404"
                android:layout_alignStart="@+id/rToggleJ404"
                android:layout_alignEnd="@+id/rToggleJ404"/>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="J4-06读取"
                android:id="@+id/rButtonJ406"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="J4-07读取"
                android:id="@+id/rButtonJ407"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="J4-08读取"
                android:id="@+id/rButtonJ408"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="J4-09读取"
                android:id="@+id/rButtonJ409"/>
        </TableRow>

    </TableLayout>

</RelativeLayout>
